{
  "theme": {
    "spacing": {
      "unit": 5
    },
    "breakpoints": {
      "small": "50rem",
      "medium": "85rem",
      "large": "105rem"
    },
    "colors": {
      "tonalOffset": 0.3
    },
    "typography": {
      "fontSize": "14px",
      "lineHeight": "1.5em",
      "fontWeightRegular": "400",
      "fontWeightBold": "600",
      "fontWeightLight": "300",
      "fontFamily": "Roboto, sans-serif",
      "smoothing": "antialiased",
      "optimizeSpeed": true,
      "headings": {
        "fontFamily": "Montserrat, sans-serif",
        "fontWeight": "400",
        "lineHeight": "1.6em"
      },
      "code": {
        "fontSize": "13px",
        "fontFamily": "Courier, monospace",
        "color": "#e53935",
        "backgroundColor": "rgba(38, 50, 56, 0.05)",
        "wrap": false
      },
      "links": {
        "textDecoration": "auto",
        "hoverTextDecoration": "auto"
      }
    },
    "sidebar": {
      "width": "260px",
      "backgroundColor": "#fafafa",
      "textColor": "#333333",
      "groupItems": {
        "textTransform": "uppercase"
      },
      "level1Items": {
        "textTransform": "none"
      },
      "arrow": {
        "size": "1.5em"
      }
    },
    "logo": {
      "gutter": "2px"
    },
    "rightPanel": {
      "backgroundColor": "#263238",
      "width": "40%",
      "textColor": "#ffffff",
      "servers": {
        "overlay": {
          "backgroundColor": "#fafafa",
          "textColor": "#263238"
        },
        "url": {
          "backgroundColor": "#fff"
        }
      }
    },
    "fab": {
      "backgroundColor": "#263238",
      "color": "#ffffff"
    }
  }
}
